    "babel-jest": "^29.4.3",
    "babel-loader": "^9.1.2",
    "browserslist": "^4.21.5",
    "c8": "^7.13.0",
    "css-loader": "^6.7.3",
    "dotenv-webpack": "^8.0.1",
